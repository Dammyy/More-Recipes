{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "node": "6.11.4"
  },
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "node ./node_modules/babel-cli/bin/babel.js ./server/ -d  dist",
    "migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "start-dev": "nodemon --watch server --exec babel-node -- server/bin/www",
    "start": "npm run build && npm run migrate && node ./dist/bin/www.js",
    "test": "mocha server/test/*.js --exit --require babel-register --reporter spec",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "start-client": "NODE_ENV=development & webpack-dev-server",
    "build-client": "NODE_ENV=build & webpack"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "http": "0.0.0",
    "json-server": "^0.12.0",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.0.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
